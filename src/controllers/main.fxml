<StackPane fx:id="stackPane" prefHeight="464.0" prefWidth="710.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.controllers.MainMenuController">
   <children>
      <ImageView fx:id="backgroundImage" fitHeight="464.0" fitWidth="665.0" pickOnBounds="true">
         <image>
            <Image url="https://image.freepik.com/free-psd/abstract-background-design_1297-87.jpg" />
         </image>
      </ImageView>
      <VBox fx:id="contentBox" alignment="CENTER" prefHeight="464.0" prefWidth="683.0" spacing="10.0" style="-fx-background-image: url('C:\Users\Joshua Harrington\Desktop\Eclipse workspace\DeathMatch\Assets\Backgrounds\Arena.jpg');;">
         <children>
            <Label fx:id="titleLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="49.0" prefWidth="329.0" text="DeathMatch" textAlignment="CENTER">
               <font>
                  <Font name="Arial Black" size="49.0" />
               </font></Label>
            <Button fx:id="newGameButton" mnemonicParsing="false" onAction="#newGameButtonAction" prefHeight="25.0" prefWidth="104.0" text="New Game" />
            <Button fx:id="loadGameButton" mnemonicParsing="false" onAction="#loadGameButtonAction" prefHeight="25.0" prefWidth="103.0" text="Load Game" />
            <Button fx:id="settingsButton" mnemonicParsing="false" onAction="#settingButtonAction" prefHeight="25.0" prefWidth="103.0" text="Settings" />
