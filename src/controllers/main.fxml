<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="stackPane" prefHeight="465.0" prefWidth="821.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.controllers.MainMenuController">
   <children>
      <ImageView fx:id="backgroundImage" fitHeight="465.0" fitWidth="821.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Assets/Backgrounds/Arena.jpg" />
         </image>
      </ImageView>
      <VBox fx:id="contentBox" alignment="CENTER" prefHeight="484.0" prefWidth="840.0" spacing="10.0">
         <children>
            <ImageView fitHeight="263.0" fitWidth="425.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../Assets/Other/title.png" />
               </image>
            </ImageView>
            <Button fx:id="newGameButton" mnemonicParsing="false" onAction="#newGameButtonAction" prefHeight="25.0" prefWidth="104.0" text="New Game" />
            <Button fx:id="loadGameButton" mnemonicParsing="false" onAction="#loadGameButtonAction" prefHeight="25.0" prefWidth="103.0" text="Load Game" />
            <Button fx:id="settingsButton" mnemonicParsing="false" onAction="#settingButtonAction" prefHeight="25.0" prefWidth="103.0" text="Settings" />
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exitButtonAction" prefHeight="25.0" prefWidth="104.0" text="Exit" />
         </children>
      </VBox>
   </children>
</StackPane>
